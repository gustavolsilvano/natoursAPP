{
  "expo": {
    "name": "NatourAPP",
    "description": "APP version of the natour site",
    "slug": "natour",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#55C57A",
    "splash": {
      "image": "./assets/splashScreen2.png",
      "resizeMode": "contain",
      "backgroundColor": "#55C57A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "node_modules/@expo/vector-icons/src/vendor/react-native-vector-icons/Fonts/Feather.ttf",
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tucdev.natours",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.tucdev.natours",
      "versionCode": 1
    }
  }
}
